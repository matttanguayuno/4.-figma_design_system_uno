<Page x:Class="figma_design_system_uno.Presentation.HeaderVisualizerPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:local="using:figma_design_system_uno.Presentation"
      xmlns:controls="using:figma_design_system_uno.Controls"
      Background="{StaticResource DefaultBackgroundBrush}"
      SizeChanged="OnPageSizeChanged">

  <Grid x:Name="RootGrid">
    <controls:HeaderControl x:Name="HeaderControlElement"
                        IsMobile="False"
                        IsMenuOpen="False" />
  </Grid>
</Page>

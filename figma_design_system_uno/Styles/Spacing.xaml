<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

  <!-- Spacing Values -->
  <x:Double x:Key="Space050">2</x:Double>
  <x:Double x:Key="Space100">4</x:Double>
  <x:Double x:Key="Space200">8</x:Double>
  <x:Double x:Key="Space300">12</x:Double>
  <x:Double x:Key="Space400">16</x:Double>
  <x:Double x:Key="Space600">24</x:Double>
  <x:Double x:Key="Space800">32</x:Double>
  <x:Double x:Key="Space1200">48</x:Double>
  <x:Double x:Key="Space1600">64</x:Double>
  <x:Double x:Key="Space4000">160</x:Double>

  <!-- Thickness Values -->
  <Thickness x:Key="Spacing050">2</Thickness>
  <Thickness x:Key="Spacing100">4</Thickness>
  <Thickness x:Key="Spacing200">8</Thickness>
  <Thickness x:Key="Spacing300">12</Thickness>
  <Thickness x:Key="Spacing400">16</Thickness>
  <Thickness x:Key="Spacing600">24</Thickness>
  <Thickness x:Key="Spacing800">32</Thickness>
  <Thickness x:Key="Spacing1200">48</Thickness>
  <Thickness x:Key="Spacing1600">64</Thickness>
  <Thickness x:Key="Spacing4000">160</Thickness>

  <!-- Border Radius -->
  <CornerRadius x:Key="Radius200">8</CornerRadius>
  <CornerRadius x:Key="RadiusFull">9999</CornerRadius>

  <!-- Stroke Width -->
  <x:Double x:Key="StrokeBorder">1</x:Double>

</ResourceDictionary>
